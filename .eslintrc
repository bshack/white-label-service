{
    "env":{
        "node":true,
        "browser":true,
        "jasmine":true,
        "amd":true,
        "es6":true
    },
    "extends":"eslint:recommended",
    "globals":{
    },
    "plugins":[
    ],
    "parserOptions":{
        "sourceType":"module"
    },
    "rules":{
        "no-console":1,
        "curly":2,
        "comma-dangle":[
            2,
            "never"
        ],
        "strict":[
            0,
            "function"
        ],
        "camelcase":[
            2,
            {
                "properties":"always"
            }
        ],
        "quotes":[
            2,
            "single"
        ],
        "no-multiple-empty-lines":2,
        "space-before-function-paren":[
            2,
            "never"
        ],
        "indent":[
            2,
            4
        ],
        "max-len":[
            2,
            120,
            4
        ],
        "max-params":[
            2,
            3
        ],
        "no-unused-vars":[
            0
        ]
    }
}
